$display-width = 176px
$bg = hsla(0, 0%, 96%, 1)
$bg2 = hsla(200, 100%, 20%, 1)
$bg_alice = hsl(336, 50%, 25%)

html{
background-color: hsl(0,0,64)
color: hsl(0,0,99)
}

